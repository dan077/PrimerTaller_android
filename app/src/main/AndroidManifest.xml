<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.primertaller">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PrimerTaller">
        <activity android:name=".GeometriaActivity"></activity>
        <activity android:name=".FuerzaActivity" />
        <activity android:name=".VelocidadActivity" />
        <activity android:name=".FisicaActivity" />
        <activity android:name=".TextoActivity" />
        <activity android:name=".RegistroActivity" />
        <activity android:name=".RestablecerActivity" />
        <activity android:name=".ValidarCodigoActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>